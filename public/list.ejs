
<html>
    <head>
        <title >List of users</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script src="https://kit.fontawesome.com/08cfd4b809.js" crossorigin="anonymous"></script>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <style>
            a{
                text-decoration: none;
            }
        </style>
    </head>
    <body >
        <br><a style="margin-left: 2%;" href="/">
        <i class="fas fa-chevron-left"> Home</i></a>
        <h4 class="text-center">List of Users</h4>
        <div class="d-flex bd-highlight mb-3">
            
            <div class="me-auto p-2 bd-highlight">
                <% if(page>1){ %><a href="/list?v=<%=page-1%>"><i class="fas fa-caret-square-left"></i> Previous</a> <% } %>
            </div>
            <div class="p-2 bd-highlight"><a href="/list?v=<%=page+1%>"> Next <i class="fas fa-caret-square-right"></i></a></div>
          </div>

        <br>
        <table class="text-center table table-dark" style="overflow-x:auto;">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Gender</th>
                <th>Status</th>
            </tr>
            <% data1.forEach(function(data){ %>
                <tr>
                    <td><%=data.id%></td>
                    <td><%=data.name%></td>
                    <td><%=data.email%></td>
                    <td><%=data.gender%></td>
                    <td><%=data.status%></td>
                </tr>
            <%})%>
        </table>
    </body>
</html>